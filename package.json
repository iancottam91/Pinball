    "babel-preset-es2015": "^6.24.1",
    "jest": "^21.2.1",
    "jsdom": "^11.5.1",
    "regenerator-runtime": "^0.11.1",
    "ts-jest": "^21.2.4",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
